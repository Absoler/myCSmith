#! /bin/bash
opt_option=
test_type=
pin_root=
root_dir=
gcc_name=
clang_name=
if [ $1 == 0 ] ; then
    compiler=$gcc_name
elif [ $1 == 1 ] ; then
    compiler=icc
else 
    compiler=$clang_name
fi
$compiler output2.c -g $opt_option -w -o output2
$pin_root/pin -t $root_dir/checkAccess/obj-intel64/checkAccess.so -- ./output2 $test_type func ./
objdump -dl output2 > disas